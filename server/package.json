{
  "name": "collabcode-server",
  "version": "1.0.0",
  "description": "Socket.IO server for CollabCode real-time collaboration",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "mkdir -p dist && tsc",
    "start": "node dist/src/index.js",
    "clean": "rimraf dist || true",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "@y/websocket-server": "^0.1.1",
    "dotenv": "^16.4.0",
    "isolated-vm": "^5.0.4",
    "rimraf": "^6.0.0",
    "socket.io": "^4.8.0",
    "uuid": "^10.0.0",
    "winston": "^3.11.0",
    "ws": "^8.17.0",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.0",
    "tsx": "^4.0.0",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "socket.io",
    "websocket",
    "real-time",
    "collaboration",
    "typescript"
  ],
  "author": "CollabCode",
  "license": "MIT"
}
