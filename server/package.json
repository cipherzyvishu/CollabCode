{
  "name": "collabcode-server",
  "version": "1.0.0",
  "description": "Socket.IO server for CollabCode real-time collaboration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npm run clean && tsc",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "socket.io": "^4.8.0",
    "dotenv": "^16.4.0",
    "@supabase/supabase-js": "^2.45.0",
    "uuid": "^10.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.0.0",
    "rimraf": "^6.0.0"
  },
  "keywords": [
    "socket.io",
    "websocket",
    "real-time",
    "collaboration",
    "typescript"
  ],
  "author": "CollabCode",
  "license": "MIT"
}
